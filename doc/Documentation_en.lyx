\begin_layout Standard
Nous utilisons le fichier de configuration suivant pour effectuer l'apprentissag
e d'un réseau Lolimot.
\end_layout

\begin_layout LyX-Code
MODELNAME Test_Cone_woNoise
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
NBMAXPART 10
\end_layout

\begin_layout LyX-Code
RESGAPPERC 0.0003
\end_layout

\begin_layout LyX-Code
SIGMA 0.22
\end_layout

\begin_layout LyX-Code
NBCUT 4
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
LEARNMETH BOOTSTRAPLIGHT 0.20 20
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
TRAIN Data/Cone_woNoise_LL.dat 0
\end_layout

\begin_layout LyX-Code
VALID Data/Cone_woNoise_LL.dat 0
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
COMPUTER2
\end_layout

\begin_layout LyX-Code
OPTIMIZESIGMA 0.1 0.01 1.0
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
% USEITERATIVEREGRESSION
\end_layout

\begin_layout LyX-Code

\end_layout

\begin_layout LyX-Code
MEASURE 0
\end_layout

\begin_layout LyX-Code
OUTPUT Y 2 1 0
\end_layout

\begin_layout LyX-Code
INPUT X1 0 1 0 0 0
\end_layout

\begin_layout LyX-Code
INPUT X2 1 1 0 1 0
\end_layout

